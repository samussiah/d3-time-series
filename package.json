{
    "name": "d3-time-series",
    "version": "1.0.1",
    "description": "Time-series library with d3.js and SVG",
    "module": "./src/index.js",
    "main": "./d3_time_series.js",
    "scripts": {
        "build": "npm run bundle && npm run format",
        "bundle": "rollup -c",
        "format": "prettier --print-width=100 --tab-width=4 --single-quote --write ./src/d3_timeseries.js",
        "uglifycss": "uglifycss src/d3_timeseries.css > dist/d3_timeseries.min.css",
        "watch": "rollup -c -w"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mcaule/d3-timeseries.git"
    },
    "bugs": {
        "url": "https://github.com/mcaule/d3-timeseries/issues"
    },
    "homepage": "https://github.com/mcaule/d3-timeseries#readme",
    "keywords": [
        "d3",
        "time series",
        "interactive"
    ],
    "author": {
        "name": "Matthieu Caule",
        "email": "matthieu.caule@gmail.com"
    },
    "license": "MIT",
    "dependencies": {
        "d3": "^4.10.0"
    },
    "devDependencies": {
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "prettier": "^1.16.4",
        "rollup": "^1.2.2",
        "rollup-plugin-babel": "^2.7.1",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-uglify": "^2.0.1",
        "uglifycss": "0.0.27"
    }
}
